<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pyhtag.appfx.MainWindowController">
   <center>
      <SplitPane fx:id="splitPane" BorderPane.alignment="CENTER">
        <items>
            <StackPane fx:id="ground">
               <children>
                  <AnchorPane fx:id="graphicLayer" />
               </children>
            </StackPane>
        </items>
      </SplitPane>
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <CheckBox fx:id="showGrid" alignment="CENTER" mnemonicParsing="false" text="Shwo grid" />
            <CheckBox fx:id="showPanelSetting" alignment="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Shwo panel setting" />
            <CheckBox fx:id="showRelativeParent" alignment="CENTER" layoutX="149.0" layoutY="10.0" mnemonicParsing="false" text="Shwo relative parent" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="8.0" top="5.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
